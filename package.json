{
  "name": "socat",
  "private": true,
  "version": "0.1.0",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "npm run apply-mig -- --remote && npm run push",
    "push": "wrangler deploy",
    "apply-mig": "wrangler d1 migrations apply socat-db",
    "init-mig": "wrangler d1 migrations create socat-db",
    "create-mig": "prisma migrate diff --from-empty --to-schema-datamodel ./prisma/schema.prisma --script",
    "prisma": "prisma generate",
    "test": "vitest run",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "pretty": "prettier src --write"
  },
  "license": "MIT",
  "dependencies": {
    "@prisma/adapter-d1": "^5.13.0",
    "@prisma/client": "^5.13.0",
    "hono": "^4.2.9",
    "status-code-enum": "^1.0.0"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.2.6",
    "@cloudflare/workers-types": "^4.20240423.0",
    "@tailwindcss/cli": "^4.0.0-alpha.14",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-istanbul": "1.3.0",
    "esbuild": "^0.20.2",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^3.2.5",
    "prisma": "^5.13.0",
    "tailwindcss": "^4.0.0-alpha.14",
    "typescript": "^5.4.5",
    "vitest": "1.3.0",
    "wrangler": "^3.53.0",
    "zod-prisma": "^0.5.4"
  }
}
